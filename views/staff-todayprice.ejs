<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <title>Merchant Register</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
 </head>

  <body>
  
  
	<form id="form" action="/test1" method="post">
        <label for="name">Today Date</label><br>
        <input type="date" name="filled_date"/><br>

        <label for="name">Time</label><br>
        <input type="time" name="filled_time"/><br>

        <label for="name">Corn Type</label><br>
        <input type="number" name="corn_type"/><br>

        <label for="name">Corn Quality</label><br>
        <select id="price" name="quality">
          <option value="Q1">1 star</option>
          <option value="Q2">2 star</option>
          <option value="Q3">3 star</option>
          
        </select><br>

        <label for="name">Today Price</label><br>
        <input type="number" name="price"/><br><br>
        

    
    <input type="submit" name="submit"/>

  </form>


  <script>
    
  const windowClose = () => {
    setTimeout(()=>{
      window.close();
    },2000);    
  }

      
  document.getElementById("form").addEventListener("submit", windowClose); 
	
 
  </script>
  </body>

</html>