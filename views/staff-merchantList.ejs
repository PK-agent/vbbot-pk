<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <title>Staff Inventory List</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
 </head>

  <body>
  <h1>Staff Add Inventory List</h1>
    
  <table cellspacing="1" cellpadding="5" border="1">
    <tr>
    
    <th>Name</th>
    <th>Phone</th>
    <th>Address</th>  
    <th>Corn Type</th>  
    <th>Quantity</th>
    <th>Wanted Price</th>
    <th>Comments</th>
    <th>Received Date</th>
    <th>Purchase Price</th>
        
  </tr>

  <% data.forEach(function(merchant){ %>
     <tr>
      
       <td><%= merchant.name %></td>
       <td><%= merchant.phone %></td>
       <td><%= merchant.address %></td>
       <td><%= merchant.corn_type %></td>
       <td><%= merchant.corn_qty %></td>
       <td><%= merchant.wanted_price %></td>
       <td><%= merchant.comment %></td>
       <td><%= merchant.received_date %></td>
       
     
       <td><a href="/staff/merchant/add-inventory/<%= merchant.id %>">Add Inventory</a> | <a href="/admin/stocklist/<%= merchant.id %>">Delete</a></td>
     </tr>
  <% }); %>
 

  </table> 


  </body>

</html>