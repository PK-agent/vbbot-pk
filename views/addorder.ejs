<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <title>Customer-Order-Form</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
 </head>

  <body>
  <h1>Add Order</h1>
  <form id="form" action="/customer/order" method="post">
  <label for="name">Orderlist Name</label><br>
    <input type="text" name="name" value="<%= data.merchant_name %>" disabled/><br>
  
    <label for="name">Quantity</label><br>
    <input type="number" name="order_qty"/><br>    

    <label for="name">Received Date</label><br>
    <input type="date" name="order_received_date"/><br>     

    <input type="hidden" name="merchant_name" value="<%= data.merchant_name %>"/>
    <input type="hidden" name="merchant_id" value="<%= data.merchant_id %>"/>

    <input type="submit" name="submit"/>

  </form>

  
  
  
  </body>

</html>